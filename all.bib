@article{dolan:2008a,
	Abstract = {The term 'utility' can be interpreted in terms of the hedonic experience of an outcome (experienced utility) or in terms of the preference or desire for that outcome (decision utility). It is this second interpretation that lies at the heart of the methods that economists have developed to value non-market goods, such as health. In this article, we argue that decision utility is unlikely to generate meaningful data on the utility associated with different experiences, and instead economists should look towards developing measures that focus more directly on experienced utility.},
	Annote = {Nice paper arguing that experienced utility measures (especially DRM) may give better valuations for health (and other non-market goods and bads) than preference-based methods. This is because members of the public underestimate the extent of adaptation (by following a 'Peak-Start Rule'), leading to overestimates, whilst those already experiencing the good or bad may neglect to take account of the past transition period, leading to underestimates.

Finds that single-measure satisfaction ratings may not be effective experienced utility indicators because of temporal and context effects.

Also: 

* Argues that once a health budget is set, it is legitimate to reduce priority given to life-enhancing treatments for conditions to which there is more adaptation, all other things being equal. (Whereas there is also an argument for increasing the priority given to life-saving/life-extending treatments in such conditions, since more QALYs will be produced by these).

* Notes that decision utilities elicited may include elements of fear of future disease in addition to dispassionate judgments as to the likely experience of the disease state.},
	Author = {Paul Dolan and Daniel Kahneman},
	Date-Added = {2008-10-07 10:30:10 +0100},
	Date-Modified = {2011-08-21 22:06:26 +0100},
	Doi = {10.1111/j.1468-0297.2007.02110.x},
	Journal = {The Economic Journal},
	Keywords = {Happiness, Health},
	Pages = {215-234},
	Printed = {Yes},
	Priority = {4},
	Rating = {4},
	Read = {Yes},
	Title = {Interpretations Of Utility And Their Implications For The Valuation Of Health},
	Volume = {118},
	Year = {2008},
	Bdsk-File-1 = {}}

