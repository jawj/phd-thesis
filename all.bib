	Keywords = {Spatial econometrics, Health},
	Month = jul,
	Number = {4},
	Pages = {591--603},
	Priority = {3},
	Title = {Deprivation Indices, Population Health and Geography: An Evaluation of the Spatial Effectiveness of Indices at Multiple Scales},
	Url = {http://dx.doi.org/10.1007/s11524-007-9193-3},
