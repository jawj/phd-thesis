@article{pope:2009,
	Abstract = {Background Exposure to fine-particulate air pollution has been associated with increased morbidity and mortality, suggesting that sustained reductions in pollution exposure should result in improved life expectancy. This study directly evaluated the changes in life expectancy associated with differential changes in fine particulate air pollution that occurred in the United States during the 1980s and 1990s. Methods We compiled data on life expectancy, socioeconomic status, and demographic characteristics for 211 county units in the 51 {U.S.} metropolitan areas with matching data on fine-particulate air pollution for the late 1970s and early 1980s and the late 1990s and early 2000s. Regression models were used to estimate the association between reductions in pollution and changes in life expectancy, with adjustment for changes in socioeconomic and demographic variables and in proxy indicators for the prevalence of cigarette smoking. Results A decrease of 10 {micro}g per cubic meter in the concentration of fine particulate matter was associated with an estimated increase in mean {({+/-}SE)} life expectancy of 0.61{+/-}0.20 year {(P=0.004).} The estimated effect of reduced exposure to pollution on life expectancy was not highly sensitive to adjustment for changes in socioeconomic, demographic, or proxy variables for the prevalence of smoking or to the restriction of observations to relatively large counties. Reductions in air pollution accounted for as much as 15\% of the overall increase in life expectancy in the study areas. Conclusions A reduction in exposure to ambient fine-particulate air pollution contributed to significant and measurable improvements in life expectancy in the United States.
},
	Author = {C. Arden Pope and Majid Ezzati and Douglas W. Dockery},
	Date-Added = {2009-01-23 15:56:20 +0000},
	Date-Modified = {2009-01-23 15:57:22 +0000},
	Doi = {{10.1056/NEJMsa0805646}},
	Journal = {N Engl J Med},
	Keywords = {Air pollution},
	Number = {4},
	Pages = {376--386},
	Priority = {5},
	Title = {{Fine-Particulate} Air Pollution and Life Expectancy in the {United States}},
	Url = {http://content.nejm.org/cgi/content/abstract/360/4/376},
	Volume = {360},
	Year = {2009},
	Bdsk-File-1 = {}}

@article{pepper:1998,
	Abstract = {The commonly discussed forms of lsquostrongrsquo sustainable development are rooted in either ecocentric or anthropocentric perspectives. Both eco-socialism and social ecology constitute anthropocentric (or lsquohomocentricrsquo) strong sustainability: being essentially humanistic but also embracing stewardship of nature, environmentally benign but centred on social justice. The bases of this development model are reviewed. Its desirability is argued from the perspective of a socialist critique of the alternative anthropocentrically-grounded version of lsquosustainabilityrsquo that is widely supported today, known as lsquoecological modernizationrsquo. It is argued that ecological modernization is at best only weakly sustainable. Indeed, contemporary Marxist readings of capitalism's inherent contradictions suggest that this development model is not sustainable at all in the long run. Global modernization, whether purportedly lsquoecologicalrsquo or not, is likely to foster continued attempts at displacement and externalization of both environmental and social costs. Current corporate reactions against both environmentalism and social democracy are therefore unsurprising. {\^O}{\o} 1998 John Wiley \& Sons, Ltd and {ERP} Environment.},
