	Priority = {4},
	Title = {Structural Equation Modeling {(SEM)} for Satisfaction and Dissatisfaction Ratings; Multiple Group Invariance Analysis Across Scales with Different Response Format},
	Url = {http://dx.doi.org/10.1007/s11205-008-9278-8},
