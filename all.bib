@article{praag:1971,
	Abstract = {
This paper is an empirical investigation into the validity of some theses posed in [5]. There it is stated (1) that the individual is able to evaluate his income level on a zero-one scale in a cardinal way, and (2) that the resulting evaluation U(y) of a steady income stream at level y under specific assumptions is approximately equal to [lambda] (y; [mu], [sigma]2), where [lambda] is a lognormal distribution function with parameters [mu], [sigma]2.
In this paper these statements are empirically investigated on the basis of a consumer survey conducted by the Belgian Consumer Union. Moreover, approximate relationships between the values of [mu], [sigma]2 and several variables, such as income, family size, age, job, stability of income, etc., have been derived.
It turns out that [sigma]2 is an individual constant, but that [mu] can be explained for a considerable part by the variables family size and income level. Most notable is the dependence of the individual's welfare evaluation on income, a phenomenon that is called " preference drift." It might lead to major changes in parts of the existing economic theory on consumer behavior and social welfare.},
	Author = {van Praag, B. M. S.},
	Date-Added = {2009-01-20 13:54:21 +0000},
	Date-Modified = {2009-01-20 16:32:06 +0000},
	Journal = {European Economic Review},
	Keywords = {Happiness, Income},
	Number = {3},
	Pages = {337--369},
	Title = {The welfare function of income in {B}elgium: An empirical investigation},
	Url = {http://www.sciencedirect.com/science/article/B6V64-489S7X0-4/2/a87faec9924f39fdc2ae8dc94830cd68},
	Volume = {2},
	Year = {1971},
	Bdsk-File-1 = {}}

@article{seidl:1994,
	Abstract = {
The Leyden economists contend that the individual welfare function (utility function) of income is best represented by a lognormal distribution function. They have presented ample data to document the good empirical fit of the lognormal hypothesis. This paper presents, reexamines, and criticises the theoretical foundations of the lognormal hypothesis and finds that this edifice is not built on solid ground, neither from the point of view of economic theory, nor of experimental psychology. Moreover, it investigates the paradox of the good empirical fit of the lognormal hypothesis in spite of its theoretical deficiencies. It is shown that the lognormal hypothesis is observationally equivalent to the logarithmic (power) welfare function when respondents evaluate welfare according to a normal (lognormal) distribution function.},
	Author = {Christian Seidl},
	Date-Added = {2009-01-20 13:57:46 +0000},
	Date-Modified = {2009-01-20 15:55:19 +0000},
	Doi = {10.1016/0014-2921(94)90031-0},
	Issn = {0014-2921},
	Journal = {European Economic Review},
	Keywords = {Happiness, Income},
	Month = oct,
	Number = {8},
	Pages = {1633--1659},
	Title = {How sensible is the {L}eyden individual welfare function of income?},
	Url = {http://www.sciencedirect.com/science/article/B6V64-45CWYXH-1H/2/2835b663e1a0c7b89162b7c875ea252f},
