@article{greenhalgh:2005,
	Abstract = {I argue that free-market capitalist economies are biased against inventing/using green technology and against supplying the basic needs of the poor. With no mechanism for setting globally optimal prices for non-renewables, entrepreneurs choose labour-saving resource-intensive production methods. Further pressure on labour costs comes from finite individual lifetimes combined with rising access to goods. R\&D creates technologies/products geared to saving worker and consumer time, instead of conserving non-renewable resources. Demand for positional luxury goods by the rich crowds out the basic needs of the poor. Technology caters for the demands of the rich, accentuating inequality, as prices fall/quality rises with innovation. I conclude with policies to redress imbalances. },
	Author = {Christine Greenhalgh},
