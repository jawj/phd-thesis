	Rating = {4},
	Read = {Yes},
	Title = {Spatial autocorrelation and neighborhood quality},
	Url = {http://www.sciencedirect.com.gate2.library.lse.ac.uk/science/article/B6V89-45KNB23-X/2/5db99dc52804c42b74a960e21501b126},
