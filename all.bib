	Id = {9158},
	Isbn = {00031224},
	Journal = {American Sociological Review},
	Keywords = {Survey methods},
	Number = {2},
	Pages = {144--162},
	Title = {Informed Consent: Consequences for Response Rate and Response Quality in Social Surveys},
	Ty = {JOUR},
